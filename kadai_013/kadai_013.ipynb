        "# 消費税を加えた合計金額を返す関数\n",
        "def calculate_total(price, tax):\n",
        "    total = price + tax\n",
        "    # 戻り値で結果を返す\n",
        "    return total\n",
        "\n",
        "# 関数の呼び出し\n",
        "price = 110\n",
        "tax = 10\n",
        "total_amount = calculate_total(price, tax)\n",
        "\n",
        "# 結果の表示\n",
        "print(total_amount)\n"
